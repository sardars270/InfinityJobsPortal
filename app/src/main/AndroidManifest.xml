            android:label="@string/title_activity_my_jobs"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".add_exp" />
        <activity android:name=".Splash" />
        <activity android:name=".ClientSignUp" />
        <activity android:name=".ListOfExperienceActiviy">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".admin.AdminMainActivity"
            android:label="@string/title_activity_admin_main" />
    </application>

</manifest>